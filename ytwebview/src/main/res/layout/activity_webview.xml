<com.maogousoft.ytwebview.PullToRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/refresh_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include layout="@layout/refresh_head" />
    <!-- 支持所有实现Pullable接口的View -->

    <com.maogousoft.ytwebview.view.SafeWebView
        android:id="@+id/sWebView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <include layout="@layout/load_more" />

</com.maogousoft.ytwebview.PullToRefreshLayout>